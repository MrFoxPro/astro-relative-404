{
   "name": "astro-relative-404",
   "version": "1.0.2",
   "description": "Using relative 404.astro of every route instead of root",
   "keywords": [
      "astro",
      "404"
   ],
   "author": "foxpro tuningiposadka@gmail.com",
   "license": "MIT",
   "homepage": "https://github.com/MrFoxPro/astro-relative-404",
   "bugs": "https://github.com/MrFoxPro/astro-relative-404/issues",
   "exports": {
      "node": {
         "import": "./dist/es/index.mjs",
         "require": "./dist/cjs/index.cjs"
      },
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.cjs"
   },
   "scripts": {
      "build": "rollup -c",
      "types": "tsc"
   },
   "files": [
      "./dist"
   ],
   "main": "./dist/cjs/index.cjs",
   "module": "./dist/es/index.mjs",
   "types": "./dist/index.d.ts",
   "devDependencies": {
      "@babel/core": "^7.19.6",
      "@babel/preset-typescript": "^7.18.6",
      "@rollup/plugin-babel": "^6.0.2",
      "@rollup/plugin-node-resolve": "^15.0.1",
      "prettier": "^2.7.1",
      "rollup": "^2",
      "rollup-plugin-delete": "^2.0.0",
      "typescript": "^4.7.4"
   },
   "peerDependencies": {
      "astro": "^1"
   }
}
